gen.tab <- function(er) {
# Generate selected error rates table
# er: error rate table
# return value: selected error rates table
# Models tested in this error rate table
model.tested = c()
tab = matrix(0, nrow = 50, ncol = length(levels(er$Model)))
count = 0
for (model in model.order) {
idx = which((er$Model == model) & (er$If_Selected == 1))
if (length(idx)>0) {
count=count+1
model.tested = append(model.tested, model)
tmp = er[idx,]$Error_Rate
tab[,count] = as.numeric(as.character(tmp))
}
}
tab = as.data.frame(tab)
colnames(tab) <- model.tested
return(tab)
}
